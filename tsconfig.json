{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "strict": true,
    "target": "es5",
    "module": "commonjs",
    "lib": ["dom", "es2017"],
    "jsx": "react", // or "react-jsx"
    "moduleResolution": "node",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
